{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "numReplicas": 1,
    "restartPolicyType": "ON_FAILURE",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 30,
    "startCommand": null
  },
  "environments": {
    "production": {
      "variables": {
        "ENVIRONMENT": "production",
        "DEBUG": "false",
        "LOG_LEVEL": "info",
        "WORKERS": "1",
        "COOKIE_SECURE": "true",
        "COOKIE_SAMESITE": "lax",
        "MAX_CONNECTIONS": "100"
      }
    }
  },
  "regions": ["us-west1"],
  "mounts": [],
  "networking": {
    "serviceDomains": []
  }
}